{
  "name": "my-gitpay",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "node": "18.20.8",
    "npm": "10.8.2"
  },
  "scripts": {
    "migrate": "node dist/migrate.js up",
    "migrate:dev": "tsx src/migrate.ts up",
    "migrate:update": "node dist/migrate.js update",
    "migrate:update:dev": "tsx src/migrate.ts update",
    "migrate:test": "cross-env NODE_ENV=test node dist/migrate.js up",
    "migrate:test:dev": "cross-env NODE_ENV=test tsx src/migrate.ts up",
    "create:db:dev": "sequelize db:create --config src/config/config.json --env development",
    "create:db:test:dev": "sequelize db:create --config src/config/config.json --env test",
    "seed": "cross-env TYPE=seed node dist/migrate.js up",
    "seed:dev": "cross-env TYPE=seed tsx src/migrate.ts up",
    "seed:test": "cross-env TYPE=seed NODE_ENV=test node dist/migrate.js up",
    "seed:test:dev": "cross-env TYPE=seed NODE_ENV=test tsx src/migrate.ts up",
    "seed:windows:dev": "cross-env TYPE=seed tsx src/migrate.ts up",
    "seed:test:windows:dev": "cross-env TYPE=seed NODE_ENV=test tsx src/migrate.ts up",
    "migrate:test:rollback": "cross-env NODE_ENV=test node migrate.js prev",
    "migrate:test:rollback:dev": "cross-env NODE_ENV=test tsx src/migrate.ts prev",
    "rollback": "node dist/migrate.js prev",
    "rollback:dev": "tsx src/migrate.ts down",
    "rollback:test": "cross-env NODE_ENV=test node dist/migrate.js prev",
    "rollback:test:dev": "cross-env NODE_ENV=test tsx src/migrate.ts prev",
    "reset": "node dist/migrate.js reset-hard",
    "reset:dev": "tsx src/migrate.ts reset-hard",
    "reset:test": "cross-env NODE_ENV=test node dist/migrate.js reset-hard",
    "reset:test:dev": "cross-env NODE_ENV=test tsx src/migrate.ts reset-hard",
    "report": "cross-env NODE_ENV=production node dist/scripts/reports/reports.js",
    "report:dev": "tsx src/scripts/reports/reports.js",
    "build": "tsc -p tsconfig.json",
    "start:dev": "nodemon --watch src --ext ts,js --exec \"tsx src/index.ts\"",
    "start": "node ./dist/index.js",
    "test": "cross-env NODE_ENV=test mocha --require ./test/bootstrap.cjs --extension ts,js --timeout 30000 --exit \"test/**/*.test.{js,ts}\"",
    "test:e2e": "playwright test",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prepush": "npm run lint-fix",
    "heroku-postbuild": "cd frontend && npm install && npm run production && npm run sitemap && cd .. && npm run build && npm run migrate",
    "stripe:webhook": "stripe listen --forward-to localhost:3000/webhooks/stripe-platform --forward-connect-to localhost:3000/webhooks/stripe-connect",
    "simulate:webhook": "NODE_ENV=test node ./scripts/webhook_script.js",
    "typecheck": "tsc --noEmit",
    "mail:preview": "tsx src/scripts/mail/mail_test.ts",
    "sync:payment_requests": "tsx src/scripts/sync/sync_payment_requests.ts",
    "sync:payouts": "tsx src/scripts/payouts/payout_sync.ts",
    "reports:revenue": "tsx src/scripts/reports/revenue-reports/revenue_report.ts",
    "reports:financial_summary": "tsx src/scripts/reports/financial-summary/financial_summary.ts",
    "reports:balance_summary": "tsx src/scripts/reports/balance-summary/balance_summary.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "cd frontend && npm run translate"
    }
  },
  "private": true,
  "dependencies": {
    "@paypal/paypal-server-sdk": "^1.1.0",
    "@sendgrid/mail": "^6.5.5",
    "bcrypt": "^5.1.0",
    "bitbucket-api-v2": "^0.6.2",
    "bitbucket-v2": "^0.4.3",
    "bluebird": "^3.5.3",
    "body-parser": "^1.20.2",
    "bootstrap": "^3.4.1",
    "color": "^3.1.2",
    "compression": "^1.7.4",
    "core-js": "^2.4.1",
    "cron": "^1.4.0",
    "cross-env": "^7.0.3",
    "dateformat": "^3.0.3",
    "decimal.js": "^10.4.3",
    "dotenv": "^4.0.0",
    "eslint-config-taller": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "express": "^5.1.0",
    "express-session": "^1.15.2",
    "feed-read": "0.0.1",
    "heroku-ssl-redirect": "0.0.4",
    "i18n": "^0.8.3",
    "init": "^0.1.2",
    "intl": "^1.2.5",
    "invert-color": "^2.0.0",
    "ip": "^1.1.5",
    "jquery": "^3.4.1",
    "jsonwebtoken": "^9.0.0",
    "mailchimp-api-v3": "^1.12.1",
    "moment": "^2.22.2",
    "name-initials": "^0.1.3",
    "nanoid": "^3.1.23",
    "natives": "^1.1.6",
    "node-okbitbucket": "^1.0.10",
    "octonode": "^0.9.5",
    "passport": "^0.6.0",
    "passport-bitbucket": "^2.0.0",
    "passport-bitbucket-oauth2": "^0.1.2",
    "passport-bitbucket-oauth20": "^1.1.0",
    "passport-facebook": "^2.1.1",
    "passport-github2": "^0.1.10",
    "passport-google-oauth20": "^1.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-stub": "^1.1.1",
    "pg": "^8.9.0",
    "pg-hstore": "^2.3.4",
    "query-string": "^5.1.0",
    "request-promise": "^4.2.0",
    "rxjs": "^5.1.0",
    "sass": "^1.77.8",
    "sendgrid": "^5.2.3",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.0",
    "sequelize-heroku": "^1.0.0",
    "stripe": "^14.12.0",
    "tslib": "^2.8.1",
    "tslint": "~6.1.3",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "umzug": "^3.8.2",
    "url": "^0.11.0",
    "url-search-params": "^1.0.2",
    "x-frame-options": "^1.0.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.0",
    "@types/bluebird": "^3.5.42",
    "@types/body-parser": "^1.19.6",
    "@types/chai": "^5.2.3",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.4",
    "@types/i18n": "^0.13.12",
    "@types/jasmine": "2.5.38",
    "@types/node": "^18.19.130",
    "@types/request-promise": "^4.1.51",
    "@types/supertest": "^6.0.3",
    "@types/umzug": "^2.3.9",
    "chai": "^3.5.0",
    "chai-spies": "^1.0.0",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^0.14.3",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^11.7.1",
    "mockdate": "^2.0.2",
    "nock": "^14.0.5",
    "nodemon": "^2.0.20",
    "prettier": "^3.6.2",
    "request": "^2.88.0",
    "should": "^11.2.1",
    "sinon": "^21.0.0",
    "supertest": "^7.1.1"
  },
  "description": "![Gitpay](https://alexandremagno.net/wp-content/uploads/2019/05/gitpaydesigntop.png)",
  "main": "cron.js",
  "directories": {
    "test": "test"
  },
  "repository": {
