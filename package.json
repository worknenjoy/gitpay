{
  "name": "my-gitpay",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "node": "18.20.8",
    "npm": "10.8.2"
  },
  "scripts": {
    "migrate": "node migrate.js up",
    "migrate-update": "node migrate.js update",
    "migrate-test": "cross-env NODE_ENV=test node migrate.js up",
    "seed": "TYPE=seed node migrate.js up",
    "seed-test": "TYPE=seed NODE_ENV=test node migrate.js up",
    "seed:windows": "cross-env TYPE=seed node migrate.js up",
    "seed-test:windows": "cross-env TYPE=seed NODE_ENV=test node migrate.js up",
    "migrate-test-rollback": "NODE_ENV=test node migrate.js prev",
    "report": "NODE_ENV=production node report.js",
    "rollback": "node migrate.js prev",
    "rollback-test": "NODE_ENV=test node migrate.js prev",
    "reset:dev": "node migrate.js reset-hard",
    "reset:test": "NODE_ENV=test node migrate.js reset-hard",
    "start:dev": "nodemon ./index.js --ignore '/frontend/*'",
    "start": "node --max-old-space-size=480 ./index.js",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --timeout 30000 --exit test/*.test.js",
    "test:e2e": "playwright test",
    "build-css": "node-sass --include-path scss src/assets/sass/material-dashboard.scss src/assets/css/material-dashboard.css",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "prepush": "npm run lint-fix",
    "postbuild": "cd frontend && npm run test",
    "heroku-postbuild": "cd frontend && npm install --legacy-peer-deps && npm run production && npm run sitemap && cd .. && npm run migrate",
    "stripe:webhook": "stripe listen --forward-to localhost:3000/webhooks",
    "simulate:webhook": "NODE_ENV=test node ./scripts/webhook_script.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "cd frontend && npm run translate"
    }
  },
  "private": true,
